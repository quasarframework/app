{
  "name": "{{ name }}",
  "version": "1.0.0",
  "description": "{{ description }}",
  "productName": "{{ productName }}",
  "cordovaId": "{{ cordovaId }}",
  "author": "{{ author }}",
  "private": true,
  "scripts": {
    {{#lint}}"lint": "eslint --ext .js,.vue src",{{/lint}}
    {{#test}}"test": "mocha-webpack quasar.config.js test/*"{{/test}}
  },
  "dependencies": {
    {{#axios}}"axios": "^0.18.0"{{/axios}}{{#i18n}}{{#axios}},{{/axios}}
    "vue-i18n": "^7.3.3"{{/i18n}}
  },
  "devDependencies": {
    {{#lint}}
    "babel-eslint": "^8.2.1",
    "eslint": "^4.18.2",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-vue": "^4.3.0",
    {{#if_eq lintConfig "standard"}}
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    {{/if_eq}}
    {{#if_eq lintConfig "airbnb"}}
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    {{/if_eq}}
    {{/lint}}
    "quasar-cli": "^0.15.14",
    {{#test}}
    "@vue/test-utils": "^1.0.0-beta.15",
    "jsdom": "^11.9.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.1.1",
    "mocha-webpack": "^1.1.0",
    "chai": "^4.1.2"
   {{/test}}
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
